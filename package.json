        "react-slick": "^0.14.11",
        "react-tap-event-plugin": "^2.0.1",
        "react-transition-group": "^2.2.0",
        "recompose": "0.21",
        "redux": "4",
        "redux-form": "^7.4.2",
        "redux-logger": "^3.0.6",
        "redux-observable": "1",
        "redux-optimistic-ui": "^3.0.0",
        "regenerator-runtime": "^0.13.2",
        "rxjs": "6",
        "sass-loader": "^6.0.6",
        "slugify": "^1.3.4",
        "style-loader": "^0.13.1",
        "svg-loader": "^0.0.2",
