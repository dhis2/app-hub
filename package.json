{
    "name": "@dhis2/app-store",
    "version": "1.0.0",
    "main": "src/main.js",
    "author": "Erik Arenhill <erik@dhis2.org>",
    "license": "BSD-3-Clause",
    "private": true,
    "scripts": {
        "build": "webpack -p --config ./webpack.config.js",
        "start": "NODE_ENV=production node ./src/main.js",
        "dev": "USE_PREBUILT_APP=1 concurrently --kill-others npm:dev:*",
        "dev:server": "DEBUG=knex:tx,appstore* NODE_ENV=development nodemon --watch ./src --inspect src/main.js",
        "dev:client": "NODE_ENV=development webpack-dev-server",
        "test": "NODE_ENV=test npm run reset-and-seed-db && lab -a @hapi/code -t 80 -v -I 'core,__core-js_shared__' test/index.js",
        "test-coverage": "NODE_ENV=test lab -c -a code -r html -o coverage/coverage.html test/index.js",
        "reset-db": "npx knex migrate:rollback && npx knex migrate:latest",
        "seed-db": "npx knex seed:run",
        "reset-and-seed-db": "npx knex migrate:rollback && npx knex migrate:latest && npx knex seed:run",
        "cypress:open": "cypress open"
    },
    "dependencies": {
        "@babel/core": "^7.6.2",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "@hapi/hapi": "^18.4.0",
        "@hapi/joi": "^16.1.7",
        "auth0-lock": "^10.18.0",
        "aws-sdk": "^2.547.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-imports": "^1.5.1",
        "blipp": "^3.1.2",
        "boom": "^7.3.0",
        "classnames": "^2.2.5",
        "concurrently": "^4.1.2",
        "copy-webpack-plugin": "^5.0.3",
        "core-js": "^3.1.4",
        "css-loader": "^0.26.1",
        "debug": "^4.1.1",
        "dotenv": "^6.2.0",
        "file-loader": "^3.0.1",
        "hapi-auth-jwt2": "^8.7.0",
        "hapi-pino": "^6.1.0",
        "hapi-swagger": "^10.1.2",
        "history": "^4.5.1",
        "html-webpack-plugin": "^3.2.0",
        "inert": "^5.1.2",
        "jwt-decode": "^2.1.0",
        "knex": "^0.17.3",
        "lodash": "^4.17.15",
        "material-components-web": "^0.16.0",
