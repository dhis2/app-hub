{
    "name": "@dhis2/app-hub",
    "version": "1.0.0",
    "main": "src/main.js",
    "author": "Erik Arenhill <erik@dhis2.org>",
    "license": "BSD-3-Clause",
    "private": true,
    "scripts": {
        "build": "NODE_ENV=production webpack -p --config ./webpack.config.js",
        "start": "NODE_ENV=production node ./src/main.js",
        "dev": "cross-env USE_PREBUILT_APP=1 concurrently --kill-others npm:dev:*",
        "dev:server": "cross-env DEBUG=knex:tx,apphub* NODE_ENV=development nodemon --watch ./src --inspect src/main.js",
        "dev:client": "cross-env NODE_ENV=development webpack-dev-server",
        "test": "npm run test:server && npm run test:client",
        "test:server": "cross-env NODE_ENV=test npm run reset-and-seed-db && lab -a @hapi/code -t 80 -v -I 'core,__core-js_shared__,k'",
        "test:client": "cross-env NODE_ENV=test mocha --opts client/test/mocha.opts",
        "test-coverage": "cross-env NODE_ENV=test npm run reset-and-seed-db && lab -c -a @hapi/code -r html -o coverage/coverage.html",
        "reset-db": "npx knex migrate:rollback && npx knex migrate:latest",
        "seed-db": "npx knex seed:run",
        "reset-and-seed-db": "npx knex migrate:rollback && npx knex migrate:latest && npx knex seed:run",
        "cypress:open": "cypress open"
    },
    "dependencies": {
        "@babel/core": "^7.6.2",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
