{
  "name": "appstore-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "webpack-dev-server --config app/webpack.config.js",
    "toolbox": "react-toolbox-themr"
  },
  "devDependencies": {
    "babel-core": "6",
    "babel-loader": "6",
    "babel-polyfill": "6",
    "babel-preset-es2015": "6",
    "babel-preset-react": "6",
    "babel-preset-stage-2": "6",
    "react-toolbox-themr": "^1.0.2",
    "redux-logger": "2.8.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "material-components-web": "^0.4.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.17.0",
    "node-sass": "^4.5.0",
    "react": "15",
    "react-dom": "15",
    "react-flexbox-grid": "^0.10.2",
    "react-redux": "5",
    "react-router": "next",
    "react-router-dom": "next",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "^2.0.0-beta.6",
    "recompose": "0.21",
    "redux": "3",
    "redux-observable": "0.12",
    "rxjs": "5",
    "sass-loader": "^5.0.1",
    "style-loader": "^0.13.1"
  },
  "reactToolbox": {
    "include": [
      "AVATAR",
      "APP_BAR",
      "BUTTON",
      "CARD",
      "DATE_PICKER",
      "DRAWER",
      "DROPDOWN",
      "LIST",
      "LINK",
      "MENU",
      "NAVIGATION",
      "INPUT",
      "PROGRESS_BAR",
      "RIPPLE",
      "SNACKBAR",
      "LAYOUT",
      "TOOLTIP"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-blue-a200)",
      "color-accent-dark": "var(--palette-blue-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "./app/src/styles/react-toolbox"
  }
}
